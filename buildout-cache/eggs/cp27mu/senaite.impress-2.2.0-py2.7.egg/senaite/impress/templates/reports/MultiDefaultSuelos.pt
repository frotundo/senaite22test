<tal:report
  i18n:domain="senaite.impress"
 define="collection view/collection;
          laboratory view/laboratory;
          accredited_symbol string:★;
          outofrange_symbol string:⚠;
          footer python:view.get_footer_text();
          report_options python:options.get('report_options', {});
          attachments_per_row python:int(report_options.get('attachments_per_row', 2));
          attachments_per_row python:attachments_per_row<1 and 1 or attachments_per_row;
          page_width options/page_width|nothing;
          page_height options/page_height|nothing;
          content_width options/content_width|nothing;
          content_height options/content_height|nothing;">

  <!-- Custom Report Controls -->
  <div id="controls" class="noprint">
    <div i18n:translate="" class="text-secondary mb-2">Opciones de informes personalizados</div>
    <!-- Attachments per row -->
    <div class="mb-3">
      <div class="input-group">
        <div class="input-group-prepend">
          <label class="input-group-text" for="attachments_per_row" i18n:translate="">
           Adjunto por Filas
          </label>
        </div>
        <input tal:attributes="value attachments_per_row"
               type="number"
               class="form-control"
               name="attachments_per_row"
               min="1"/>
      </div>
      <small class="form-text text-muted" i18n:translate="">
        Número de archivos adjuntos representados dentro de una fila por solicitud de análisis
      </small>
    </div>
  </div>

  <script type="text/javascript">
   console.info("######################################################################");
   window.options = "<tal:t replace='options'/>";
   console.log(window.options);
   console.info("######################################################################");
  </script>

  <tal:css>
    <style type="text/css">
     .report * { font: 8pt; }
     .report * { font: 8pt; }
     .report h1 { font-size: 70%; }
     .report h2 { font-size: 90%; }
     .report h3 { font-size: 80%; }
     .report h5 { font-size: 70%; }
     .report .font-size-140 { font-size: 100%; }
     .report .font-size-120 { font-size: 100%; }
     .report .font-size-100 { font-size: 100%; }
     .report .colon-after:after { content: ":"; }
     .report address { margin: 1rem 0; }
     .report table.noborder td, .report table.noborder th { border: none; }
     .report table.nopadding td { padding: 0; }
     .report table td.label { padding-right: 0.3rem; font-weight: bold; }
     .report table { border-color: #898a8b; }
     .report table td, .report table th { border-top: 1px solid #898a8b; border-bottom: 1px solid #898a8b; }
     .report table th { border-bottom: 1px solid #898a8b; }
     .report table.range-table td { padding: 0 0.3rem 0 0; border: none; }
     .report .section-header h1 { font-size: 100%;}
     .report .section-header img.logo { position: fixed; height: 100px; margin: 0px 0; }

     .report .section-header img.img-fluid { position: fixed;text-align: right;height: 65px; margin-top: 50px 0; }
     .report .barcode-hri { margin-top: -0.25em; font-size: 8pt; }
     .report .section-footer table td { border: none; }
     .report .section-footer {
       position: fixed;
       left: -10mm;
       bottom: -10mm;
       margin-left: 10mm;
       margin-top: 10mm;
       height: 10mm;
       width: 100%;
       text-align: left;
       font-size: 8pt;
     }
     .report .section-footer #footer-line {
       width: 100%;
       height: 2mm;
       border-top: 1px solid #898a8b;
     }

     <tal:block condition="python:content_width and content_height">
     <tal:block condition="python:all([content_width, content_height])"
                   define="cw python:float(content_width);
                           ch python:float(content_height);">
     /* Ensure that the images stay within the borders */
     .report .section-attachments img {
       max-width: <tal:t replace="python:'{:.2f}mm'.format(cw / attachments_per_row)"/>;
       max-height: <tal:t replace="python:'{:.2f}mm'.format(ch * 0.75)"/>;
     }
     .report .section-resultsinterpretation img {
       max-width: <tal:t replace="python:'{:.2f}mm'.format(cw)"/>;
       max-height: <tal:t replace="python:'{:.2f}mm'.format(ch * 0.75)"/>;
     }

   </tal:block>
     @page {
       @bottom-right {
         vertical-align: top;
         margin-top: 2mm;
         font-size: 7pt;
         content: "<tal:t i18n:translate=''>Página</tal:t> " counter(page) " <tal:t i18n:translate=''>de</tal:t> " counter(pages);
       }
     }
     </style>
    </tal:css>
 <!-- HEADER -->
<tal:render condition="python:len(collection)>0"
              define="model python:collection[0];
                      batch model/Batch;">
    <tr class="row section-header no-gutters">
             <td>
              <table class="table noborder">
                <colgroup>
                    <col style="width:50%"/>
                    <col style="width:50%;"/>
                </colgroup>
      <!-- Header Table -->
                 <div class="align-middle text-right">
                   <img class="logo" style= "max-width:200px; position: fixed;margin:0px 5px; margin-top: -16mm;"
                   tal:attributes="src string:++plone++senaite.core.static/images/logo_print.png"/>
                </div>
                 <div  style ="max-lines: 100%;position: fixed; text-align: right; margin: 100px 600px; margin-top: -16mm;">
                       <h1><b><p> FILAB-2001
                              Rev.01
                           25-08-2021</p></b></h1>
                </div>
                 <div style ="width: 40%;align-content:left; margin: 100px 470px; margin-top: -4.5mm; position: fixed">
                   <h2><b> INFORME Nº:</b></h2>
                 </div>
                     <!-- Barcode -->
                 <td class="barcodaine-conter mt-2">
                      <div class="clearfix ">
                       <div class="text-right barcode align=center"
                            style = "max-width:110px;position: fixed;margin: 200px 530px; margin-top: -12mm;"
                             data-code="code128"
                             data-showHRI="true"
                             data-barHeight="7.5"
                             data-addQuietZone="false"
                             tal:attributes="data-id batch/id" 
                             tal:content="model/VerSampled"/>
                        </div>
                 </td>
                </table>

              </td>
            </tr>

   </tal:render>


  <!-- INFO (First AR is primary for the data) -->

<tal:render condition="python:len(collection)>0"
              define="model python:collection[0];
                      batch model/Batch;
                      client model/Client;
                      contact model/Contact;
                      laboratory python:view.laboratory;">
             <md-tabs-dummy-wrapper class="align-middle text-center">
                   <div class="alert alert-info" tal:condition="model/is_provisional">
                           <div i18n:translate=""><b>Reporte Preliminar</b></div>
                   </div>
                    <h2 i18n:translate=""><b>INFORME DE ENSAYOS</b></h2>
                    <h2 class="label" i18n:translate="">Fecha Emisión: <span tal:content="python:here.toLocalizedTime(view.timestamp)"></span></h2>

             </md-tabs-dummy-wrapper>

<br><br>
     <div class="row section-info no-gutters">
      <div class="w-100">

        <!-- Client Info -->

         <table class="table table-sm table-condensed w-100">
             <colgroup>
                  <!-- Tipo-->
                  <col style="width: 40%;">
                  <!-- Detalle -->
                  <col style="width: 60%">
             </colgroup>
            <h2><b>ANTECEDENTES TITULAR</b></h2>
            <td>
            <address class="laboratory-address">
              <div class="text-left">
                  <h2 i18n:translate=""><b>Titular</b></h2>
              </div>
              <div class="text-left">
                   <h2 i18n:translate=""><b>Dirección</b></h2>
              </div>
              <div class="text-left">
                   <h2 i18n:translate=""><b>Rut</b></h2>
              </div>
              <div class="text-left">
                   <h2 i18n:translate=""><b>Fuente o Actividad</b></h2>
              </div>
                <div class="text-left">
                   <h2 i18n:translate=""><b>Nombre del Proyecto</b></h2>
              </div>
 </address>


  </td>
              <td>
              <address class="laboratory-address">
                <div class="client-name">
                    <h2><b>:</b> <div tal:replace="client/Name|nothing"></div></h2>
                </div>
                 <div class="client-address">
                 <h2> <b>:</b> <div tal:replace="contact/PostalAddress/address|nothing"></div></h2>
                </div>
                 <div class="Rut">
                <h2> <b>:</b> <div tal:replace="client/ClientID|nothing"></div></h2>
                </div>
                <div class="contact-fullname">
                  <h2><b>:</b> <div tal:replace="client/TaxNumber|nothing"></div></h2>
                </div>
                  <div class="proyecto-fullname">
                  <h2><b>:</b> <div tal:replace="model/SamplePoint/description|nothing"></div></h2>
                </div>
         </address>

         </table>

        <table class="table table-sm table-condensed w-100">
              <colgroup>
                  <!-- Tipo-->
                  <col style="width: 40%;">
                  <!-- Detalle -->
                  <col style="width: 60%">

                </colgroup>
           <h2><b>ANTECEDENTES ETFA</b></h2>

            <td>
              <div class="text-left">
                  <h2 i18n:translate=""><b>Empresa</b></h2>
            </div>
                <div class="text-left">
                   <h2 i18n:translate=""><b>Dirección</b></h2>
            </div>
            <div class="text-left">
                   <h2 i18n:translate=""><b>Código ETFA</b></h2>
            </div>

             <div class="text-left">
                   <h2 i18n:translate=""><b>Acreditación INN</b></h2>
            </div>
              <div class="text-left">
                   <h2 i18n:translate=""><b>Inspectores Ambientales</b></h2>
            </div>
            </td>
            <td>
                <div class="name">
                    <h2><b>:</b> Algoritmos y Mediciones Ambientales SPA </h2>
                </div>
                 <div class="address">
                 <h2> <b>:</b> Seminario Nº180, Providencia, Santiago</h2>
                </div>
                 <div class="Etfa">
                <h2> <b>:</b>015-01</h2>
                </div>
                <div class="acreditation">
                  <h2><b>:</b> LE 1078 - LE1079 - LE1080 </h2>
                </div>
                 <div class="IAmbiental">
                  <h2><b>:</b> Jocelyne Catalán Neira Código IA: 16.680.002-1  </h2>
                   <h2><b> </b> Carlos Fernández Código IA: 7.983.534-K  </h2>

                </div>

            </td>

        </table>
   </div>

        <table class="table table-sm table-condensed w-100">
              <colgroup>
                  <!-- Tipo-->
                  <col style="width: 40%;">
                  <!-- Detalle -->
                  <col style="width: 60%">

                </colgroup>

           <h2><b>ANTECEDENTES DE LAS MUESTRAS</b></h2>
            <td>
              <div class="text-left">
                  <h2 i18n:translate=""><b>Tipo de Muestra</b></h2>
            </div>
             <div class="text-left">
                   <h2 i18n:translate=""><b>Instrumento ambiental aplicable</b></h2>
            </div>
            <div class="text-left">
                   <h2 i18n:translate=""><b>Fecha y Hora Início/Termino Muestreo</b></h2>
            </div>

            </td>
              <td>
                <div class="name">
                   <h2> <b>:</b> <div tal:replace="model/SampleType/title|nothing"></div></h2>
                </div>
                 <div class="var2">
                   <tal:by_client_ref repeat="client_ref python:view.group_items_by('ClientReference', collection)">
                     <h2> <b>:</b>  <div tal:replace="client_ref"></div></h2>
                   </tal:by_client_ref>
                 </div>
                  <div class="Fechas">
                   <h2> <b>:</b>  <div tal:replace="batch/description|nothing"></div></h2>
                  </div>
              </td>

        </table>

      <!-- Clear Floats
           https://github.com/Kozea/WeasyPrint/issues/36
      -->
      <div class="clearfix"></div>
    </div>
  </tal:render>

  <!-- ALERTS -->
  <tal:render condition="python:True">
    <div class="row section-alerts no-gutters">
      <div class="w-100">
        <tal:model repeat="model collection">
          <div class="alert alert-danger" tal:condition="model/is_invalid">
            <h2 class="alert-heading"><span tal:replace="model/getId"/></h2>
            <div i18n:translate="">Esta solicitud de análisis ha sido invalidada debido a resultados publicados erróneamente</div>
            <tal:invalidreport tal:define="child model/Retest"
                               tal:condition="child">
              <span i18n:translate="">Esta solicitud de análisis ha sido reemplazada por</span>
              <a tal:attributes="href child/absolute_url"
                 tal:content="child/getId"></a>
            </tal:invalidreport>
          </div>
        </tal:model>
      </div>
    </div>
  </tal:render>

<br><br>
  <!-- RESULTS -->
 <tal:render condition="python:True">
    <tal:model repeat="model collection">
      <div class="row section-results no-gutters">
       <table class="table noborder">
        <colgroup>
            <col style="width:50%"/>
            <col style="width:50%;"/>
          </colgroup>
       <td>
        <div class="w-100">
            <h2 i18n:translate><b>ID MUESTRA: <span tal:replace="model/getId"/></b></h2>
            <h2 i18n:translate><b>ID CLIENTE: <span tal:replace="model/ClientOrderNumber"/></b></h2>
            <h2 i18n:translate><b>FECHA MUESTREO: <span tal:replace="python:view.to_localized_time(model.DateSampled)"/></b></h2>
        </div>
       </td>
            <td>
            <div class="clearfix">
                        <div class="barcode left"
                             data-code="code128"
                             data-showHRI="false"
                             data-barHeight="10"
                             data-addQuietZone="false"
                             tal:attributes="data-id model/getId">
                        </div>
            </div>
           </td>
       </table>

          <!-- Point of Capture -->
          <tal:poc define="analyses_by_poc python:view.get_analyses_by_poc(model);"
                   tal:repeat="poc analyses_by_poc">

            <!-- Analysis Category -->
            <tal:categories_in_poc define="categories_by_poc python:view.get_categories_by_poc(model)"
                                   repeat="category python:view.sort_items(categories_by_poc.get(poc))">

              <!-- Analysis in POC and Category -->
              <table class="table table-sm table-condensed">
                <colgroup>
                  <!-- Category -->
                  <col style="width: 26%;">
                   <!-- Fechas -->
                  <col style="width: 16%;">
                     <!-- Fechas -->
                  <col style="width: 16%;">
                  <!-- Result -->
                  <col style="width: 11%">
                    <!-- LD -->
                <col style="width: 10%">
                  <!-- Unit -->
                  <col style="width: 8%">

                </colgroup>
                <thead>
                  <tr class="small">
                    <th class="analysis">
                      <span tal:content="category/Title">Category</span>
                    </th>
                     <th class="text-center date">
                      <span i18n:translate="">F. Inicio</span>
                    </th>
                    <th class="text-center date">
                      <span i18n:translate="">F. Final</span>
                    </th>
                    <th class="text-right result">
                      <span i18n:translate="">Resultado</span>
                    </th>
                     <th class="text-center LD">
                      <span i18n:translate="">LD</span>
                    </th>
                    <th class="text-right unit">
                      <span i18n:translate="">Unidad</span>
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <tal:analyses tal:repeat="analysis python:view.get_analyses_by(model, poc=poc , category=category)">
                    <tr>
                      <td>
                      <h6>
                       <span tal:condition="python:analysis.title[ - 5 :] != '(EXT)' ">
                      <span tal:condition="python:analysis.title[ 0:5] != 'Prome' ">
                       <span tal:condition="python:analysis.title[ 0:5] != 'Volum' ">
                        <span tal:condition="python:analysis.title[ 0:5] != 'Fecha' ">
                         <span tal:condition="not:analysis/Accredited">
                          <span tal:condition="not:analysis/AccreditedINN">
                              <span tal:condition="not:analysis/AccreditedA2LA">
                                 <span class="text-success"
                                 tal:content="accredited_symbol"
                                    style="font-family:Lucida Console, Courier, monospace;"></span>
                              </span>
                          </span>
                         </span>
                        </span>
                       </span>
                      </span>
                       </span>
                        <span tal:condition="not:analysis/ScientificName">
                          <span class="font-weight-normal" style="font-size:8pt;" tal:content="analysis/title"></span>
                        </span>
                        <span tal:condition="analysis/ScientificName">
                         <h2 <span class="font-italic" style="font-size:8pt;" tal:content="analysis/title"></span>
                        </span>
                      </td>
                      <td class="text-left">
                           <h2><span class="Date" tal:content="python:view.to_localized_time(analysis.getStartProcessDate()) if analysis.getStartProcessDate() < analysis.ResultCaptureDate else view.to_localized_time(analysis.ResultCaptureDate)"></span></h2>
                      </td>
                      <td class="text-left">
                        <h2><span class="Date" tal:content="python:view.to_localized_time(analysis.getDateVerified())"></span></h2>

                      </td>
                      <td class="text-right">
                        <h2> <span tal:content="structure python:model.get_formatted_result(analysis)">23</span> </h2>
                      </td>
                        <td class="text-center">
                            <h2><span tal:content="structure python:model.get_formatted_ld(analysis)"></span></h2>
                      </td>
                      <td class="text-right">
                       <h2> <span tal:content="structure python:model.get_formatted_unit(analysis)"></span></h2>
                      </td>
                    </tr>
                  </tal:analyses>
                </tbody>
                <tfoot tal:define="category_comments python:category.Comments">
                  <tr tal:condition="category_comments">
                    <td colspan="3">
                      <div class="category_comments"
                           tal:content="category_comments">
                        Category Comments
                       </div>
                    </td>
                  </tr>
                </tfoot>
              </table>
            </tal:categories_in_poc>
            </tal:poc>
        </div>

    </tal:model>
  </tal:render>

<!-- Metodologia -->
          <tal:render condition="python:len(collection)>0"
              define="model python:collection[0];
                      batch model/Batch;">
            <tal:poc define="analyses_by_poc python:view.get_analyses_by_poc(model);"
                   tal:repeat="poc analyses_by_poc">
             <table class="w-100 mb-0 noborder" >
                <colgroup>
                  <!-- Title -->
                    <col style="width: 30%;">
                    <!-- Result -->
                <col style="width: 70%">
                 </colgroup>
                 <thead>
                     <tr class="font-weight-bold table-info small">
                          <th class="analysis">
                            <span i18n:translate="">Análisis</span>
                          </th>
                          <th class="text-left methods">
                            <span i18n:translate="">Metodología</span>
                          </th>
                     </tr>
                 </thead>
                 <tal:analyses tal:repeat="analysis python:view.get_analyses_by(model, poc=poc)">
                      <tal:poc tal:repeat="poc analyses_by_poc">
                         <tbody>

                            <td class="analysis">
                               <span tal:condition="analysis/getMethodTitle">
                                <span class="font-weight-normal" style="font-size:8pt;" tal:condition="not:analysis/ScientificName">

                                      <span   tal:content="analysis/title"></span>
                                </span>
                                <span tal:condition="analysis/ScientificName">
                                    <span tal:content="analysis/title"></span>
                                </span>
                               </span>
                            </td>

                            <td class="text-left methods">
                                 <span class="font-weight-normal" style="font-size:8pt;"  tal:condition="analysis/getMethodTitle">
                                     <span tal:replace="analysis/getMethodTitle"></span>
                                </span>
                            </td>

                         </tbody>
                      </tal:poc>
                 </tal:analyses>
              </table>
            </tal:poc>
         </tal:render>

             <!--  RESULTS INTERPRETATIONS -->
  <tal:render condition="python:True">
    <tal:model repeat="model collection">
      <div class="row section-resultsinterpretation no-gutters"
           tal:define="ris python:model.get_resultsinterpretation();
                       has_ri python:any(map(lambda r: r.get('richtext'), ris));"
           tal:condition="ris">
        <div class="" tal:condition="has_ri">
          <h1 i18n:translate>Interpretación de resultados para <span tal:replace="model/getId"/></h1>
          <tal:ri repeat="ri ris">
            <h2 tal:condition="ri/richtext|nothing" tal:content="ri/title|nothing">Departmento</h2>
            <div class="text-info" tal:content="structure ri/richtext|nothing"></div>
          </tal:ri>
        </div>
      </div>
    </tal:model>
  </tal:render>

  <!--  REMARKS -->
    <tal:render condition="python:True">
    <tal:model repeat="model collection">
      <div class="row section-remarks no-gutters" tal:condition="model/Remarks">
        <div class="">
          <h2 i18n:translate>Observaciones para <span tal:replace="model/getId"/></h2>
          <div class="text-info" tal:content="structure model/Remarks"></div>
        </div>
      </div>
    </tal:model>
  </tal:render>

  <!-- ATTACHMENTS -->
  <tal:render condition="python:True">
    <tal:model repeat="model collection">
      <div class="row section-attachments no-gutters">
        <tal:attachment tal:define="attachments python:model.get_sorted_attachments('r');">
          <h2 i18n:translate=""
              tal:condition="attachments">
            Attachments for <span tal:replace="model/getId"/>
          </h2>
          <table class="table w-100" tal:condition="attachments">
            <colgroup tal:condition="python:len(attachments) > 1">
              <col tal:repeat="col python:range(attachments_per_row)"
                   tal:attributes="style python:'width:{}%'.format(100/attachments_per_row)">
            </colgroup>
            <tr tal:repeat="chunk python:view.group_into_chunks(attachments, attachments_per_row)">
              <td class="align-bottom"
                  style="border:none;padding-left:0;"
                  tal:repeat="attachment chunk">
                <figure class="figure">
                  <img class="figure-img img-fluid"
                       tal:attributes="src string:${attachment/absolute_url}/AttachmentFile;"/>
                  <figcaption class="figure-caption pt-2">
                    <div class="att_for">
                      <span i18n:translate="">Adjunto para</span>
                      <span tal:content="attachment/getTextTitle|model/getId"/>
                    </div>
                    <div class="att_keys">
                      <span tal:content="attachment/AttachmentKeys"/>
                    </div>
                    <div class="att_filename">
                      <span i18n:translate="">Nombre del Archivo:</span>
                      <span tal:content="attachment/AttachmentFile/filename"/>
                    </div>
                  </figcaption>
                </figure>
              </td>
            </tr>
          </table>
        </tal:attachment>
      </div>
    </tal:model>
  </tal:render>

<tal:render condition="python:len(collection)>0"
              define="model python:collection[0];
                      batch model/Batch;">
    <div class="row section-discreeter no-gutters">
      <div class="w-100 text small">
            <h2 i18n:translate>LD:Límite Detección </h2>
        <div class="outofrange">
          <span class="outofrange text-danger"
                style="font-family: Verdana, Arial, Helvetica, sans-serif"
                tal:content="outofrange_symbol">
          </span>
          <span i18n:translate="">Resultado fuera del rango especificado.</span>
        </div>
        <div class="discreeter-copyright"
             i18n:translate="">
          El informe no puede ser reproducido total o parcialmente sin autorización por escrito del laboratorio
          <tal:block replace="laboratory/title" i18n:name="name_lab"/>
        </div>
        <div class="discreeter-disclaimer"
             i18n:translate="">
            Los resultados obtenidos son válidos solo para las muestras analizadas.
        </div>
        <div class="discreeter-disclaimer"
             i18n:translate="">
            El tipo de preservante utilizado corresponde al indicado por la normativa vigente.
        </div>
        <div class="discreeter-disclaimer"
             i18n:translate="">
            Acreditación INN: LE 1078, LE 1079 y LE 1080.
        </div>
        <div class="discreeter-disclaimer"

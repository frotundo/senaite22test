          <!-- Point of Capture -->
          <tal:poc define="analyses_by_poc python:view.get_analyses_by_poc(model);"
                   tal:repeat="poc analyses_by_poc">

            <!-- Analysis Category -->
            <tal:categories_in_poc define="categories_by_poc python:view.get_categories_by_poc(model)"
                                   repeat="category python:view.sort_items(categories_by_poc.get(poc))">

              <!-- Analysis in POC and Category -->
              <table class="table table-sm table-condensed">
                <colgroup>
                 <!-- Category -->
                  <col style="width: 40%;">
                 <!-- Result -->
                  <col style="width: 10%">
                 <!-- LD -->
                  <col style="width: 10%">
                    <!-- LD -->
                  <col style="width: 10%">
                 <!-- Unit -->
                  <col style="width: 10%">
                 <!-- Fechas -->
                  <col style="width: 20%;">

                </colgroup>
                <thead>
                  <tr class="small">
                    <th class="analyses">
                      <span tal:condition="python: category.Title != 'MPS' " tal:content="category/Title">Category</span>
                    </th>
                     <th class="text-right result">
                      <span i18n:translate="">Resultado</span>
                    </th>
                     <th class="text-right LDM">
                      <span i18n:translate="">LDM</span>
                    </th>
                       <th class="text-right LCM">
                      <span i18n:translate="">LCM</span>
                    </th>
                      <th class="text-right unit">
                      <span i18n:translate="">Unidad</span>
                    </th>
                    <th class="text-center date">
                      <span i18n:translate="">Fecha Análisis</span>
                    </th>
                   </tr>
                </thead>
                 <tbody>
                  <tal:analyses tal:repeat="analysis python:view.get_analyses_by(model, poc=poc , category=category)">

                  <tr>
                    <td>
                      <h6>
                       <span tal:condition="python:analysis.title[ - 5 :] != '(EXT)' ">
                      <span tal:condition="python:analysis.title[ 0:5] != 'Prome' ">
                       <span tal:condition="python:analysis.title[ 0:5] != 'Volum' ">
                        <span tal:condition="python:analysis.title[ 0:5] != 'Fecha' ">
                         <span tal:condition="not:analysis/Accredited">
                          <span tal:condition="not:analysis/AccreditedINN">
                              <span tal:condition="not:analysis/AccreditedA2LA">
                                 <span class="text-success"
                                 tal:content="accredited_symbol"
                                    style="font-family:Lucida Console, Courier, monospace;"></span>
                              </span>
                          </span>
                         </span>
                        </span>
                       </span>
                      </span>
                       </span>
                        <span tal:condition="not:analysis/ScientificName">
                          <span class="font-weight-normal" style="font-size:8pt;" tal:content="analysis/title"></span>
                        </span>
                        <span tal:condition="analysis/ScientificName">
                         <h2 <span class="font-italic" style="font-size:8pt;" tal:content="analysis/title"></span>
                        </span>
                      </td>
                       <td class="text-right">
                       <b><span class="font-weight-normal " style="font-size:8pt;"  tal:content="structure python:model.get_formatted_result(analysis)">23</span> </b>
                      </td>
                      <td class="text-right" tal:condition="analysis/LDCSN">
                        <span class="font-weight-normal " style="font-size:8pt;" tal:content="structure python:model.get_formatted_ldc(analysis)"></span>
                      </td>
                      <td class="text-right" tal:condition="analysis/LDCSN">
                        <span class="font-weight-normal " style="font-size:8pt;"  tal:content="structure python:model.get_formatted_ld(analysis)"></span>
                      </td>
                      <td class="text-right" tal:condition="not:analysis/LDCSN">
                        <span class="font-weight-normal " style="font-size:8pt;" tal:content="structure python:model.get_formatted_ld(analysis)"></span>
                      </td>
                      <td class="text-right" tal:condition="not: analysis/LDCSN">
                        <span class="font-weight-normal " style="font-size:8pt;" tal:content="structure python:model.get_formatted_ldc(analysis)"></span>
                      </td>
                       <td class="text-left" tal:condition="python: model.get_value_fecamb(analysis)">
                          <h2 <span class="font-weight-normal" style="font-size:8pt;" tal:content="python: model.get_value_fecamb(analysis)"></span>
                      </td>
                       <td class="text-right">
                       <span class="font-weight-normal " style="font-size:8pt;"  tal:content="structure python:model.get_formatted_unit(analysis)"></span>
                      </td>
                      <td class="text-right">
                          <span i18n:translate="" style="font-size:7.5pt;"> Inicio:</span><span class="font-weight-normal" style="font-size:7pt;"  tal:content="python:view.to_localized_time(analysis.getStartProcessDate()) if analysis.getStartProcessDate() < analysis.ResultCaptureDate else view.to_localized_time(analysis.ResultCaptureDate)"></span>
                           <span i18n:translate="" style="font-size:7.5pt;"> Final:</span><span class="font-weight-normal" style="font-size:7pt;"  tal:content="python:view.to_localized_time(analysis.getDateVerified())"></span>
                      </td>

                    </tr>
                  </tal:analyses>
                </tbody>
                <tfoot tal:define="category_comments python:category.Comments">
